<div class="col hide-on-small-only m3 l2">
  <% if user_signed_in? %>
    <div class="row">
      <div class="col s12">
       <ul class="tabs tabs-fixed-width">
         <li class="tab col s3"><a href="#Chat">
             <i class="material-icons">chat</i>Chat Room
           </a>
         </li>
         <li class="tab col s3"><a href="#Fileserver">
             <i class="material-icons">cloud</i>File Server
           </a>
         </li>
       </ul>
     </div>
     <div id="Chat" class="col s12">
       <%= render :partial => "shared/chat_room" %>
     </div>
     <div id="Fileserver" class="col s12">
       <%= render :partial => "shared/file_server" %>
     </div>
   </div>
  <% end %>
</div>
